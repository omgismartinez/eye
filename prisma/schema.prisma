generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider     = "mysql"
  url          = env("DATABASE_URL")
  relationMode = "prisma"
}

model User {
  id          String   @id @default(cuid())
  external_id String   @unique
  metadata    Json
  created_at  DateTime @default(now())
  updated_at  DateTime @updatedAt
}
